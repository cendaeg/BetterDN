{
  "manifest_version": 2,
  "name": "BetterDN",
  "description": "Add features to Designer News",
  "version": "1.2",
  "icons": {"128": "logo.png" },
  "permissions": [
    "tabs",
    "notifications",
    "https://news.layervault.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://news.layervault.com/*"],
      "css":["dn.css"],
      "js": ["jquery.js", "url.js", "general.js", "editor.js", "markdown.js", "pocket.js", "follow.js", "upvotes.js", "unvisited.js", "comments.js"]
    }
  ]
}
